        # Predict
        rf_val = rf.predict(input_df)[0]
        xgb_val = xgb.predict(input_df)[0]
        ensemble_val = (w_rf * rf_val) + (w_xgb * xgb_val)

        strength_mpa = 10 ** ensemble_val
