            result_var.set("Select precursor category")
            return

        # Prepare input dictionary
        input_dict = {
            "Bulk density (g/cm³)": bulk_density,
            "Porosity (%)": porosity,
            "Carbonization temperature (˚C)": carbon_temp,
            "Heating rate (˚C/min)": heating_rate,
